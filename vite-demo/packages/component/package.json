{
  "name": "happy-vue",
  "version": "0.0.1",
  "license": "MIT",
  "description": "a component library for Vue 3 base on happy-ui",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "umd/index.js",
  "jsdelivr": "umd/index.js",
  "author": "hvuejs",
  "types": "types/index.d.ts",
  "style": "lib/index.css",
  "keywords": [
    "vue",
    "vue-component",
    "happy-vue",
    "happy"
  ],
  "exports": {
    ".": {
      "import": "./es/index.js"
    },
    "./lib/": "./lib/",
    "./es/": "./es/"
  },
  "scripts": {
    "build": "npm run build:es && npm run build:lib && npm run build:umd",
    "build:es": "vite build --emptyOutDir --config ./script/build.es.ts",
    "build:umd": "vite build --emptyOutDir --config ./script/build.umd.ts",
    "build:lib": "vite build --emptyOutDir --config ./script/build.lib.ts"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "vite-plugin-dts": "^1.3.1",
    "sass": "^1.26.10"
  }
}
